<%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%=listTitle%>
        </h1>
        <div class="box">
            <%for(var i=0;i<newListItem.length; i++){
                if(newListItem[i]!=""){%>
                <div class="item">
                    <input type="checkbox">
                    <p>
                        <%=newListItem[i]%>
                    </p>
                </div>
                <%}}%>

                    <form class="" action="/" method="post">
                        <input type="text" name="newItem" id="newItem" placeholder="New Item" autofocus autocomplete="off">
                        <button type="submit" class="submit" name="list" value="<%=listTitle%>">+</button>
                    </form>
        </div>
    </div>
    <%- include("footer") -%>